{
  "name": "covid-backend",
  "version": "1.0.0",
  "description": "covid backend application",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "compile": "tsc --project tsconfig.json",
    "compile:watch": "npm run compile -- --watch",
    "lint": "tslint -c tslint.json '{src,test}/**/*.ts'",
    "postinstall": "npm run compile",
    "dev": "concurrently 'npm run compile:watch' 'npm run start:watch'",
    "start": "node  ./dist/app.js",
    "start:watch": "nodemon -w dist -e js --exec 'npm run start'",
    "pretest": "npm run compile"
  },
  "dependencies": {
    "@types/xml": "^1.0.4",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cross-env": "^5.2.0",
    "dotenv-safe": "^5.0.1",
    "express": "^4.16.3",
    "handlebars": "^4.1.2",
    "morgan": "^1.9.1",
    "ramda": "^0.26.1",
    "walk-sync": "^0.3.3",
    "xml": "^1.0.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.5",
    "@types/chai-as-promised": "^7.1.0",
    "@types/chai-datetime": "^0.0.32",
    "@types/compression": "^0.0.36",
    "@types/express": "^4.16.0",
    "@types/helmet": "^0.0.41",
    "@types/jwt-decode": "^2.2.1",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/node": "^8.10.29",
    "@types/ramda": "^0.26.8",
    "@types/sequelize": "^4.27.25",
    "@types/sinon": "^7.0.11",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-datetime": "^1.5.0",
    "concurrently": "^4.1.0",
    "factory-girl": "^5.0.2",
    "husky": "^2.3.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "license": "ISC",
  "private": true
}
